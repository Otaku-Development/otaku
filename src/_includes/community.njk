<div id="Community" class="Community Bg--blue">
  <div class="Community__Title">
    <h2>{{comunity.title| safe}}</h2>
  </div>
  <div class="Community__Image">
    {% image "./src/assets/images/talk-1.jpg", comunity.image_alt %}
    {# {% image "./src/assets/images/talk-2.jpg", "Dan Leivers on Stage" %} #}
  </div>
  <div class="Community__Text">
    <p>{{comunity.intro| safe}}</p>
    <ul>
      <li>iOS Dev UK - 2018</li>
      <li>Swift Heros - 2018</li>
      <li>nsLondon - 2018</li>
      <li>SWmobile - 2014 - Present</li>
    </ul>
    <div class="Swmobile">
      <h3 class="sr-only">{{comunity.swmobile_title| safe}}</h3>
      {% image "./src/assets/images/swmobile.svg", "South West Mobile" %}
      <p>{{comunity.swmobile_intro| safe}}</p>
      <a href="https://www.meetup.com/swmobile/" class="Button Button--inverted" target="_blank" rel="noopener">{{comunity.swmobile_button}}</a>
    </div>
  </div>
</div>
